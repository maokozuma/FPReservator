<p id="notice"><%= notice %></p>

<h1>Fps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fps.each do |fp| %>
      <tr>
        <td><%= fp.name %></td>
        <td><%= fp.email %></td>
        <td><%= link_to 'Show', fp %></td>
        <td><%= link_to 'Edit', edit_fp_path(fp) %></td>
        <td><%= link_to 'Destroy', fp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fp', new_fp_path %>
